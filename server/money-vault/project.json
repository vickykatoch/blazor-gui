{
  "name": "money-vault",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "server/money-vault/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/server/money-vault",
        "main": "server/money-vault/src/main.ts",
        "tsConfig": "server/money-vault/tsconfig.app.json",
        "assets": [
          "server/money-vault/src/assets"
        ],
        "webpackConfig": "server/money-vault/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "money-vault:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "money-vault:build:development"
        },
        "production": {
          "buildTarget": "money-vault:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "server/money-vault/jest.config.ts"
      }
    }
  },
  "tags": []
}
